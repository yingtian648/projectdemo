<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="5dp"/>
            <solid android:color="@color/btn_pressed_bg"/>
            <stroke android:color="@color/alert_btn_box" android:width="0.5dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="5dp"/>
            <solid android:color="@color/course_num_after"/>
            <stroke android:color="@color/alert_btn_box" android:width="0.5dp"/>
        </shape>
    </item>
</selector>