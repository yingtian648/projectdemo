<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mine.demo1">

    <uses-permission android:name="android.permission.INTERNET" />
    <!-- Include next permission if you want to allow UIL to cache images on SD card -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".EApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- 注册 接收网络状态广播 -->
        <receiver android:name=".util.CheckConnectBroadcast">
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>

        <activity android:name=".MainActivity" />
        <activity android:name=".loadbackshow.LoadActivity" />
        <activity android:name=".demoLogin.SecoundActivity" />
        <activity android:name=".demoLogin.ThirdActivity" />
        <activity android:name=".demoLogin.FirstActivity" />
        <activity android:name=".head.ChooseImgActivity" />
        <activity android:name=".circleprogress.CircleProgressbarActivity" />
        <activity android:name=".calender.CalenderTestActivity" />
        <activity android:name=".pulltorefresh.PullToRefreshActivity" />
        <activity android:name=".checkbox_in_radiogroup.CheckBoxInRadiogroupActivity" />
        <activity android:name=".dialog.DialogDemoActivity"></activity>
        <activity android:name=".imageprocess.ImageProcessActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".two_dimension_code.TwoDimenCodeActivity"></activity>
    </application>

</manifest>